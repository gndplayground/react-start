    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "copyfiles": "^1.2.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.5",
    "eslint": "4.11.0",
    "eslint-config-airbnb": "^16.0.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
